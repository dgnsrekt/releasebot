{
  "name": "releasebot",
  "version": "0.9.0",
  "main": "main.js",
  "scripts": {
    "start": "node main.js start",
    "nodemodev": "nodemon main.js start --schedule \"0 * * * * *\"",
    "dev:server": "nodemon main.js start --schedule \"* * * 31 2 *\"",
    "debug": "nodemon --inspect main.js start --schedule \"0 * * * * *\" --debug",
    "format": "prettier {src,public}/**/*.{js,ts,json,md,scss} --write",
    "format:check": "prettier {src,public}/**/*.{js,ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "esm": {
    "mode": "auto",
    "cjs": true
  },
  "dependencies": {
    "esm": "^3.0.20",
    "express": "^4.16.3",
    "http-debug": "^0.1.2",
    "log4js": "^2.5.3",
    "mongoose": "^5.0.10",
    "node-fetch": "^2.1.1",
    "node-schedule": "^1.3.0",
    "now-env": "^3.0.4",
    "oauth": "^0.9.15",
    "puppeteer": "^1.2.0",
    "query-params": "0.0.1",
    "semver": "^5.5.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.1.0",
    "nodemon": "^1.17.3",
    "prettier": "^1.12.1",
    "standard-version": "^4.3.0"
  },
  "description": "Release Butler - Releases of popular frontend libraries & frameworks",
  "author": "Tomas Trajan<tomas.trajan@gmail.com>",
  "license": "MIT"
}
